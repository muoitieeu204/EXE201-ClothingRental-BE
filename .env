# Copy this file to `.env` before running docker compose

# Host ports
API_HOST_PORT=8080
SQL_HOST_PORT=14333

# SQL Server
SQL_DB_NAME=ClothingRentalDb
SQL_SA_PASSWORD=12345

# JWT
JWT_TOKEN=DEV_ONLY_CHANGE_THIS_TO_LONG_RANDOM_SECRET
